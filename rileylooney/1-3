void userinput(str, strcpr);
int stringcpr(str, strcpr);

int main() {
	char str[1001] = { NULL }, strcpr[1001] = { NULL };
	userinput(str, strcpr);
	stringcpt(str, strcpr);
}

void userinput(char* str, char* strcpr) {
	printf("문자열 입력");
	gets(str);

	pritnf("찾을 문자열 입력");
	gets(strcpr);
}

int stringcpr(char* str, char* strcpr) {
	for (int i = 0; i < strlen(str); i++) {
		printf("%c", *(str + i));
	}
}

